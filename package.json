{
  "name": "cobblestone",
  "version": "0.0.16",
  "private": false,
  "description": "A very opinionated static site generator",
  "bugs": {
    "url": "https://github.com/will-stone/cobblestone/issues",
    "email": "will@wstone.io"
  },
  "repository": {
    "url": "https://github.com/will-stone/cobblestone"
  },
  "license": "MIT",
  "author": {
    "name": "Will Stone",
    "email": "will@wstone.io",
    "url": "https://wstone.io"
  },
  "bin": {
    "cobblestone": "lib/index.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "demo:build": "cd demo && NODE_ENV=production node ../lib/index.js",
    "demo:serve": "cd demo && node ../lib/index.js",
    "release": "np",
    "test": "exit 0"
  },
  "prettier": "@will-stone/prettier-config",
  "eslintConfig": {
    "extends": [
      "@will-stone/eslint-config",
      "@will-stone/eslint-config/node",
      "@will-stone/eslint-config/react"
    ],
    "rules": {
      "no-console": "off",
      "node/global-require": "off"
    }
  },
  "dependencies": {
    "@apexearth/copy": "^1.4.5",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/register": "^7.12.1",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@sindresorhus/is": "^4.0.0",
    "@tinyhttp/app": "^0.5.34",
    "autoprefixer": "^10.0.2",
    "chalk": "^4.1.0",
    "chokidar": "^3.4.3",
    "cssnano": "^4.1.10",
    "del": "^6.0.0",
    "figures": "^3.2.0",
    "fs-extra": "^9.0.1",
    "globby": "^11.0.1",
    "html-minifier": "^4.0.0",
    "node-html-parser": "^2.0.0",
    "postcss": "^8.1.9",
    "pretty-ms": "^7.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sirv": "^1.0.7",
    "tailwindcss": "^2.0.1",
    "time-span": "^4.0.0",
    "ws": "^7.4.0"
  },
  "devDependencies": {
    "@will-stone/eslint-config": "^1.29.0",
    "@will-stone/prettier-config": "^3.1.1",
    "eslint": "^7.14.0",
    "np": "^7.0.0",
    "prettier": "^2.1.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
